{
  "version": 2,
  "builds": [
      {
          "src": "server.js",
          "use": "@vercel/node",
          "config": {
              "includeFiles": [
                  "views/**",
                  "public/**",
                  "modules/**"
              ]
          }
      }
  ],
  "routes": [
      {
          "src": "/css/(.*)",
          "dest": "/public/css/$1"
      },
      {
          "src": "/(.*)",
          "dest": "server.js"
      }
  ],
  "buildCommand": "npm run build"
}